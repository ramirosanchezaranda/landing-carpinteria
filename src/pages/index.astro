---
// Importaciones de librerías y componentes necesarios
import Layout from '../layouts/Layout.astro';
import 'swiper/css';
import 'swiper/css/navigation';
import 'swiper/css/pagination';
import Swiper from 'swiper/bundle';
import { Navigation, Pagination, Autoplay } from 'swiper/modules';
---

<Layout title="Carpintería Profesional">
    {/* 
        Sección de Navegación: Barra de navegación superior
        - Contiene logo, enlaces de navegación y botón de menú móvil
        - Implementa efecto de sticky y cambio de fondo al hacer scroll
    */}
    <nav class="fixed top-0 left-0 w-full bg-white/80 backdrop-blur-sm z-50 shadow-sm transition-all duration-300 ease-in-out">
        <div class="container mx-auto px-4 py-3 flex justify-between items-center">
            <div class="absolute left-4 top-1/2 transform -translate-y-1/2 flex items-center">
                <a href="/" class="block mr-4">
                    <img 
                        src="/lca-navbar.png" 
                        alt="LCA Carpintería Logo" 
                        class="w-16 h-16 object-cover lazy-load shadow-lg transition-transform duration-300 hover:scale-110"
                    />
                </a>
            </div>
            
            {/* Desktop Navigation */}
            <div class="hidden md:flex space-x-6 ml-auto">
                <a 
                    href="#inicio" 
                    class="text-gray-900 hover:text-blue-600 transition duration-300 font-semibold hover-color"
                >
                    Inicio
                </a>
                <a 
                    href="#galeria" 
                    class="text-gray-900 hover:text-blue-600 transition duration-300 font-semibold hover-color"
                >
                    Proyectos
                </a>
                <a 
                    href="#contacto" 
                    class="text-gray-900 hover:text-blue-600 transition duration-300 font-semibold hover-color"
                >
                    Contacto
                </a>
            </div>

            {/* Mobile Menu Toggle */}
            <div class="md:hidden ml-auto">
                <button 
                    id="mobile-menu-toggle" 
                    class="text-gray-900 focus:outline-none"
                >
                    <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>
                    </svg>
                </button>
            </div>
        </div>

        {/* Mobile Menu Dropdown */}
        <div 
            id="mobile-menu" 
            class="fixed top-20 left-0 w-full bg-white/95 backdrop-blur-sm shadow-md transform -translate-x-full transition-transform duration-300 ease-in-out will-change-transform md:hidden"
        >
            <div class="container mx-auto px-4 py-4 space-y-4">
                <a 
                    href="#inicio" 
                    class="block text-gray-900 hover:text-blue-600 transition duration-300 font-semibold text-center py-2 hover-color"
                >
                    Inicio
                </a>
                <a 
                    href="#galeria" 
                    class="block text-gray-900 hover:text-blue-600 transition duration-300 font-semibold text-center py-2 hover-color"
                >
                    Proyectos
                </a>
                <a 
                    href="#contacto" 
                    class="block text-gray-900 hover:text-blue-600 transition duration-300 font-semibold text-center py-2 hover-color"
                >
                    Contacto
                </a>
            </div>
        </div>
    </nav>

    <script is:inline>
        // Manejo de eventos de la barra de navegación
        document.addEventListener('DOMContentLoaded', () => {
            const mobileMenuToggle = document.getElementById('mobile-menu-toggle');
            const mobileMenu = document.getElementById('mobile-menu');
            let isMenuOpen = false;

            // Toggle de menú móvil
            mobileMenuToggle.addEventListener('click', () => {
                mobileMenu.classList.toggle('translate-x-0');
                mobileMenu.classList.toggle('-translate-x-full');
                isMenuOpen = !isMenuOpen;
            });

            // Cerrar menú móvil al hacer clic en un enlace
            const mobileMenuLinks = mobileMenu.querySelectorAll('a');
            mobileMenuLinks.forEach(link => {
                link.addEventListener('click', () => {
                    mobileMenu.classList.add('-translate-x-full');
                    mobileMenu.classList.remove('translate-x-0');
                    isMenuOpen = false;
                });
            });

            // Scroll suave para enlaces de la barra de navegación
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    
                    const targetId = this.getAttribute('href').substring(1);
                    const targetElement = document.getElementById(targetId);
                    
                    if (targetElement) {
                        targetElement.scrollIntoView({ 
                            behavior: 'smooth',
                            block: 'start'
                        });
                    }
                });
            });

            // Barra de navegación sticky con cambio de fondo al hacer scroll
            const navbar = document.querySelector('nav');
            const heroSection = document.getElementById('inicio');

            const handleScroll = () => {
                const scrollY = window.scrollY;
                const heroHeight = heroSection.offsetHeight;

                if (scrollY > 50) {
                    navbar.classList.add('bg-white', 'shadow-md');
                    navbar.classList.remove('bg-white/80', 'backdrop-blur-sm');
                } else {
                    navbar.classList.remove('bg-white', 'shadow-md');
                    navbar.classList.add('bg-white/80', 'backdrop-blur-sm');
                }
            };

            window.addEventListener('scroll', handleScroll);
            // Llamada inicial para establecer el estado correcto
            handleScroll();
        });
    </script>

    {/* 
        Sección Hero: Introducción principal del sitio web
        - Contiene imagen de fondo, título principal y llamado a la acción
        - Implementa efecto parallax para profundidad visual
    */}
    <div id="inicio" class="hero-wrapper relative h-screen w-full overflow-hidden pt-20 smooth-container">
        <div id="hero-image-container" class="absolute inset-0 z-0 transition-all duration-300 ease-out">
            <img 
                id="hero-image" 
                src="/mesa-rack.webp" 
                alt="Carpintería Profesional Hero Image" 
                class="w-full h-full object-cover transform transition-transform duration-300 lazy-load"
            />
            <div class="absolute inset-0 bg-black/50 backdrop-brightness-50"></div>
        </div>
        
        <div class="relative z-10 h-full flex flex-col justify-center items-center text-center text-white px-4 md:px-8 lg:px-16">
            <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold mb-6 max-w-4xl leading-tight drop-shadow-lg fade-enter">
                Transformamos Madera con Pasión y Precisión
            </h1>
            <p class="text-lg md:text-xl lg:text-2xl mb-10 max-w-3xl drop-shadow-md fade-enter">
                Soluciones personalizadas que dan vida a tus espacios, desde muebles únicos hasta diseños a medida.
            </p>
            <div class="flex space-x-4 fade-enter">
                <a 
                    href="#contacto" 
                    class="px-8 py-3 bg-blue-600 hover:bg-blue-700 text-white rounded-full transition-all duration-300 ease-in-out"
                >
                    Contáctanos
                </a>
            </div>
        </div>
    </div>

    <script>
        // Efecto parallax para la sección hero
        const heroImageContainer = document.getElementById('hero-image-container');
        const heroSection = document.getElementById('inicio');

        const handleParallaxScroll = () => {
            if (!heroImageContainer || !heroSection) return;

            const scrollY = window.scrollY;
            const heroHeight = heroSection.offsetHeight;

            // Efecto parallax: movimiento más lento de la imagen al hacer scroll
            const parallaxValue = scrollY * 0.5; // Ajustar este multiplicador para más o menos efecto
            heroImageContainer.style.transform = `translateY(${parallaxValue}px) scale(${1 + scrollY * 0.0005})`;

            // Desvanecimiento del contenido del hero al hacer scroll
            const heroContent = document.querySelector('#inicio .relative.z-10');
            if (heroContent) {
                const opacity = Math.max(0, 1 - (scrollY / heroHeight));
                (heroContent as HTMLElement).style.opacity = opacity.toString();
            }
        };

        window.addEventListener('scroll', handleParallaxScroll);
    </script>

    {/* 
        Sección de Servicios: Presentación de servicios ofrecidos
        - Contiene título, descripción y enlaces a secciones específicas
    */}
    <section class="bg-white py-16 lg:py-24">
        <div class="container mx-auto px-4 grid lg:grid-cols-2 gap-12 items-center">
            <div class="order-2 lg:order-1">
                <h2 class="text-3xl lg:text-4xl font-bold mb-6 text-gray-800">Mesa Rack: Diseño y Funcionalidad</h2>
                <p class="text-lg text-gray-600 mb-6">
                    Nuestro Mesa Rack es un ejemplo perfecto de cómo combinamos estética y practicidad. Diseñado para adaptarse a diversos espacios, este mueble no solo es una pieza decorativa, sino también una solución inteligente de almacenamiento.
                </p>
                <ul class="space-y-4 mb-8 text-gray-700">
                    <li class="flex items-center">
                        <svg class="w-6 h-6 mr-3 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                        </svg>
                        Madera de alta calidad seleccionada cuidadosamente
                    </li>
                    <li class="flex items-center">
                        <svg class="w-6 h-6 mr-3 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                        </svg>
                        Diseño modular que se adapta a diferentes espacios
                    </li>
                    <li class="flex items-center">
                        <svg class="w-6 h-6 mr-3 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                        </svg>
                        Acabados personalizables según tu estilo
                    </li>
                </ul>
                <a 
                    href="#contacto" 
                    class="inline-block px-8 py-3 bg-blue-600 hover:bg-blue-700 text-white rounded-full transition duration-300 ease-in-out transform hover:scale-105 shadow-lg hover-scale"
                >
                    Consultar Producto
                </a>
            </div>
            <div class="order-1 lg:order-2">
                <img 
                    src="/mesa-rack.webp" 
                    alt="Mesa Rack Detalle" 
                    class="w-full rounded-lg shadow-2xl transform transition duration-500 hover:scale-105"
                />
            </div>
        </div>
    </section>

    {/* 
        Sección de Proyectos: Presentación de proyectos destacados
        - Contiene título, descripción y carrusel de imágenes
    */}
    <section id="proyectos" class="bg-white py-16 lg:py-24 smooth-container">
        <div class="container mx-auto px-4">
            <div class="text-center mb-12">
                <h2 class="text-3xl lg:text-4xl font-bold mb-4 text-gray-800 fade-enter">
                    Nuestros Proyectos Destacados
                </h2>
                <p class="text-lg text-gray-600 max-w-3xl mx-auto fade-enter">
                    Cada proyecto es único. Descubre cómo transformamos la madera en obras maestras funcionales y elegantes.
                </p>
            </div>

            <div class="swiper proyectos-swiper">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="bg-white shadow-lg rounded-lg overflow-hidden">
                            <img 
                                src="/escritorio.webp" 
                                alt="Escritorio de Madera" 
                                class="w-full h-64 object-cover lazy-load"
                            />
                            <div class="p-6">
                                <h3 class="text-xl font-semibold mb-2 text-gray-800">Escritorio Minimalista</h3>
                                <p class="text-gray-600">Diseño moderno que combina funcionalidad y estética.</p>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="bg-white shadow-lg rounded-lg overflow-hidden">
                            <img 
                                src="/mesa-rack.webp" 
                                alt="Mesa Rack" 
                                class="w-full h-64 object-cover lazy-load"
                            />
                            <div class="p-6">
                                <h3 class="text-xl font-semibold mb-2 text-gray-800">Mesa Rack Industrial</h3>
                                <p class="text-gray-600">Diseño robusto que fusiona funcionalidad industrial con elegancia.</p>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="bg-white shadow-lg rounded-lg overflow-hidden">
                            <img 
                                src="/mesalampara1.webp" 
                                alt="Mesa Lámpara" 
                                class="w-full h-64 object-cover lazy-load"
                            />
                            <div class="p-6">
                                <h3 class="text-xl font-semibold mb-2 text-gray-800">Mesa Lámpara Vintage</h3>
                                <p class="text-gray-600">Pieza única que combina iluminación y diseño artesanal.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="swiper-pagination"></div>
                <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div>
            </div>
        </div>
    </section>

    <script>
        // Inicialización del carrusel de proyectos
        import Swiper from 'swiper/bundle';
        import { Navigation, Pagination, Autoplay } from 'swiper/modules';

        Swiper.use([Navigation, Pagination, Autoplay]);

        document.addEventListener('DOMContentLoaded', () => {
            new Swiper('.proyectos-swiper', {
                modules: [Navigation, Pagination, Autoplay],
                slidesPerView: 1,
                spaceBetween: 30,
                loop: true,
                autoplay: {
                    delay: 3000, // Cambia de slide cada 3 segundos
                    disableOnInteraction: false // Permite interacción sin detener el auto-scroll
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true
                },
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
                breakpoints: {
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 20
                    },
                    1024: {
                        slidesPerView: 3,
                        spaceBetween: 30
                    }
                }
            });
        });
    </script>

    {/* 
        Sección de Galería: Presentación de imágenes de proyectos
        - Contiene título y carrusel de imágenes
    */}
    <section id="galeria" class="bg-gray-100 py-16 lg:py-24">
        <div class="container mx-auto px-4">
            <h2 class="text-4xl font-bold mb-8 text-center scroll-reveal">Proyectos Destacados</h2>
            
            <div class="swiper">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <img src="/escritorio.webp" alt="Escritorio de Madera" class="rounded-lg shadow-lg">
                    </div>
                    <div class="swiper-slide">
                        <img src="/mesa-rack.webp" alt="Mesa Rack" class="rounded-lg shadow-lg">
                    </div>
                    <div class="swiper-slide">
                        <img src="/mesalampara1.webp" alt="Mesa Lámpara" class="rounded-lg shadow-lg">
                    </div>
                </div>
                
                <div class="swiper-pagination"></div>
                
                <div class="swiper-button-prev"></div>
                <div class="swiper-button-next"></div>
            </div>
        </div>
    </section>

    <script>
        // Inicialización del carrusel de galería
        import Swiper from 'swiper/bundle';
        import { Navigation, Pagination, Autoplay } from 'swiper/modules';

        Swiper.use([Navigation, Pagination, Autoplay]);

        document.addEventListener('DOMContentLoaded', () => {
            new Swiper('.swiper', {
                modules: [Navigation, Pagination, Autoplay],
                slidesPerView: 1,
                spaceBetween: 30,
                loop: true,
                autoplay: {
                    delay: 3000, // Cambia de slide cada 3 segundos
                    disableOnInteraction: false // Permite interacción sin detener el auto-scroll
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true
                },
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
                breakpoints: {
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 20
                    },
                    1024: {
                        slidesPerView: 3,
                        spaceBetween: 30
                    }
                }
            });
        });
    </script>

    {/* 
        Sección de Contacto: Formulario de contacto y redes sociales
        - Contiene título, formulario de contacto y enlaces a redes sociales
    */}
    <section id="contacto" class="bg-gray-100 py-16">
        <div class="container mx-auto px-4 grid md:grid-cols-2 gap-12">
            <div class="bg-white shadow-lg rounded-lg p-8">
                <h2 class="text-3xl font-bold mb-6 text-center text-gray-800">Contáctanos</h2>
                <form 
                    action="https://formspree.io/f/mleqzglb" 
                    method="POST" 
                    id="contactForm"
                    class="max-w-lg mx-auto bg-white p-8 rounded-lg shadow-lg"
                >
                    <div class="mb-4">
                        <label 
                            for="name" 
                            class="block text-gray-700 text-sm font-bold mb-2"
                        >
                            Nombre Completo
                        </label>
                        <input 
                            type="text" 
                            id="name" 
                            name="name" 
                            required 
                            placeholder="Tu nombre" 
                            class="w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500"
                        />
                    </div>
                    
                    <div class="mb-4">
                        <label 
                            for="email" 
                            class="block text-gray-700 text-sm font-bold mb-2"
                        >
                            Correo Electrónico
                        </label>
                        <input 
                            type="email" 
                            id="email" 
                            name="email" 
                            required 
                            placeholder="tu@email.com" 
                            class="w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500"
                        />
                    </div>
                    
                    <div class="mb-4">
                        <label 
                            for="phone" 
                            class="block text-gray-700 text-sm font-bold mb-2"
                        >
                            Teléfono (Opcional)
                        </label>
                        <input 
                            type="tel" 
                            id="phone" 
                            name="phone" 
                            placeholder="Tu número de teléfono" 
                            class="w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500"
                        />
                    </div>
                    
                    <div class="mb-4">
                        <label 
                            for="message" 
                            class="block text-gray-700 text-sm font-bold mb-2"
                        >
                            Mensaje
                        </label>
                        <textarea 
                            id="message" 
                            name="message" 
                            required 
                            rows="4" 
                            placeholder="Describe tu proyecto o consulta" 
                            class="w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500"
                        ></textarea>
                    </div>
                    
                    <div class="mb-4">
                        <label class="flex items-center">
                            <input 
                                type="checkbox" 
                                name="privacy" 
                                required 
                                class="mr-2 rounded text-blue-600 focus:ring-blue-500"
                            />
                            <span class="text-sm text-gray-600">
                                Acepto la política de privacidad y tratamiento de datos
                            </span>
                        </label>
                    </div>
                    
                    <div id="formStatus" class="mb-4 text-center hidden"></div>
                    
                    <button 
                        type="submit" 
                        class="w-full bg-blue-600 text-white py-3 rounded-md hover:bg-blue-700 transition duration-300 ease-in-out"
                    >
                        Enviar Mensaje
                    </button>
                </form>

                <script>
                    // Manejo del formulario de contacto
                    document.addEventListener('DOMContentLoaded', () => {
                        const form = document.getElementById('contactForm');
                        const formStatus = document.getElementById('formStatus');

                        if (!form || !formStatus) return;

                        form.addEventListener('submit', async (e) => {
                            e.preventDefault();

                            const submitButton = form.querySelector('button[type="submit"]');
                            if (!submitButton) return;

                            try {
                                // Deshabilitar botón durante el envío
                                (submitButton as HTMLButtonElement).disabled = true;
                                submitButton.classList.add('opacity-50', 'cursor-not-allowed');

                                // Limpiar estado anterior
                                formStatus.innerHTML = '';
                                formStatus.classList.add('hidden');

                                // Envío de formulario
                                const response = await fetch(form.getAttribute('action') || '', {
                                    method: 'POST',
                                    body: new FormData(form as HTMLFormElement),
                                    headers: {
                                        'Accept': 'application/json'
                                    }
                                });

                                const result = await response.json();

                                // Manejo de respuesta
                                if (response.ok) {
                                    formStatus.innerHTML = `
                                        <div class="bg-green-100 border border-green-400 text-green-700 px-4 py-3 rounded relative" role="alert">
                                            ¡Mensaje enviado con éxito! Nos pondremos en contacto pronto.
                                        </div>
                                    `;
                                    formStatus.classList.remove('hidden');
                                    (form as HTMLFormElement).reset();
                                } else {
                                    formStatus.innerHTML = `
                                        <div class="bg-red-100 border border-red-400 text-red-700 px-4 py-3 rounded relative" role="alert">
                                            Hubo un problema al enviar el mensaje. Por favor, inténtalo de nuevo.
                                        </div>
                                    `;
                                    formStatus.classList.remove('hidden');
                                }
                            } catch (error) {
                                // Manejo de errores de conexión
                                formStatus.innerHTML = `
                                    <div class="bg-red-100 border border-red-400 text-red-700 px-4 py-3 rounded relative" role="alert">
                                        Error de conexión. Verifica tu internet e inténtalo de nuevo.
                                    </div>
                                `;
                                formStatus.classList.remove('hidden');
                            } finally {
                                // Rehabilitar botón
                                (submitButton as HTMLButtonElement).disabled = false;
                                submitButton.classList.remove('opacity-50', 'cursor-not-allowed');
                            }
                        });
                    });
                </script>
            </div>
            <div class="flex flex-col justify-center items-center space-y-8">
                <div class="text-center">
                    <h3 class="text-2xl font-bold mb-4 text-gray-800">Síguenos</h3>
                    <div class="flex justify-center space-x-6">
                        <a 
                            href="https://facebook.com" 
                            target="_blank" 
                            rel="noopener noreferrer"
                            class="text-gray-700 hover:text-blue-600 transition duration-300"
                        >
                            <svg class="w-8 h-8" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                                <path d="M14 9h3l-.5 3H14v7h-3v-7H8V9h3V6.5A3.5 3.5 0 0 1 14.5 3h3v3h-3a.5.5 0 0 0-.5.5V9z"/>
                            </svg>
                        </a>
                        <a 
                            href="https://instagram.com" 
                            target="_blank" 
                            rel="noopener noreferrer"
                            class="text-gray-700 hover:text-pink-600 transition duration-300"
                        >
                            <svg class="w-8 h-8" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                                <path d="M16 11.5a4 4 0 1 1-8 0 4 4 0 0 1 8 0z"/>
                                <path d="M3 8.5V5.8A2.8 2.8 0 0 1 5.8 3h12.4A2.8 2.8 0 0 1 21 5.8v12.4a2.8 2.8 0 0 1-2.8 2.8H5.8A2.8 2.8 0 0 1 3 18.2V8.5z"/>
                                <line x1="17" y1="6.5" x2="17.01" y2="6.5"/>
                            </svg>
                        </a>
                        <a 
                            href="https://wa.me/+5493416123456" 
                            target="_blank" 
                            rel="noopener noreferrer"
                            class="text-gray-700 hover:text-green-600 transition duration-300"
